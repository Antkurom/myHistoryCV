#include <iostream>

int main()
{
    int array [] = {3, 5, 2, 9, 1};
    int size = 5;
    for (int i = 1; i < size; i++){
        int x = array[i];
        int j = i - 1;
        while(x < array[j] && j>=0){
            array[j+1] = array[j];
            j --;
        }
        array[j+1] = x;
    }
    for(int i = 0; i < size; i++)
        std::cout << array[i] << " ";
    return 0;
}

